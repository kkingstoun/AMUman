{
	"name": "amuman-frontend",
	"version": "1.0.0",
	"author": {
		"name": "Mathieu Moalic"
	},
	"license": "MIT",
	"keywords": [
		"ui",
		"components",
		"dashboard",
		"admin",
		"tailwindcss",
		"svelte",
		"svelte-kit",
		"layout"
	],
	"scripts": {
		"dev": "vite dev --host --port 3000",
		"build": "vite build",
		"preview": "vite preview --host --port 3000",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"gen-api": "npx -y swagger-typescript-api --add-readonly -p 'http://amuman-manager-dev:8000/api/schema/?format=yaml&lang=en' -o ./src/api/OpenApi.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/kit": "^2.5.0",
		"@types/luxon": "^3.4.2",
		"@types/prismjs": "^1.26.3",
		"@typescript-eslint/eslint-plugin": "^7.0.1",
		"@typescript-eslint/parser": "^7.0.1",
		"@zerodevx/svelte-toast": "^0.9.5",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"eslint-plugin-tailwindcss": "^3.14.2",
		"eslint-plugin-unused-imports": "^3.1.0",
		"flowbite": "^2.3.0",
		"flowbite-svelte": "^0.44.23",
		"flowbite-svelte-icons": "^1.4.0",
		"luxon": "^3.4.4",
		"postcss": "^8.4.35",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.1",
		"prismjs": "^1.29.0",
		"svelte": "^4.2.10",
		"svelte-check": "^3.6.4",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.1.3",
		"date-picker-svelte": "^2.11.0",
		"vitest": "^1.2.2"
	},
	"type": "module"
}